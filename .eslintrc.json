{
    "extends": "google",
    "env": {
        "es6": true,
        "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7,
        "ecmaFeatures": {
            "modules": true
        },
        "sourceType": "module"
    },
    "rules": {
        "arrow-parens": "off",
        "switch-colon-spacing": "off"       
    }
}